<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="tcc-ejbPU" transaction-type="JTA">
    <jta-data-source>jdbc/myDatasourceTCC</jta-data-source>
    <class>com.br.entity.Administrador</class>
    <class>com.br.entity.Aluno</class>
    <class>com.br.entity.AlunoQuestionario</class>
    <class>com.br.entity.AlunoQuestionarioResposta</class>
    <class>com.br.entity.Cursos</class>
    <class>com.br.entity.Empresa</class>
    <class>com.br.entity.EmpresaQuestionario</class>
    <class>com.br.entity.EmpresaQuestionarioResposta</class>
    <class>com.br.entity.Estagio</class>
    <class>com.br.entity.ModalidadeEstagios</class>
    <class>com.br.entity.Orientador</class>
    <class>com.br.entity.Palestra</class>
    <class>com.br.entity.Palestrante</class>
    <class>com.br.entity.Pergunta</class>
    <class>com.br.entity.PerguntaResposta</class>
    <class>com.br.entity.Questionario</class>
    <class>com.br.entity.QuestionarioPergunta</class>
    <class>com.br.entity.Resposta</class>
    <class>com.br.entity.Seminario</class>
    <class>com.br.entity.Usuario</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.driver" value="com.MySQL.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/tcc"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.password" value="1234"/>
    </properties>
  </persistence-unit>
</persistence>
